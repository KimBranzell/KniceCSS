{
  "name": "knice-css",
  "description": "A CSS framework that's knice and knifty, and totally free to use!",
  "version": "1.0.0",
  "main": "knice.scss",
  "keywords": [
    "css",
    "sass",
    "scss",
    "responsive",
    "framework",
    "scss-framework",
    "css-framework"
  ],
  "maintainers": [
    {
      "name": "Kim Branzell",
      "email": "hello@branzell.kim",
      "web": "https://branzell.kim"
    }
  ],
  "bugs": {
    "web": "https://github.com/KimBranzell/knicecss/issues"
  },
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:KimBranzell/knicecss.git"
    }
  ],
  "licenses": [
    {
      "name": "CC0 1.0 Universal",
      "url": "https://creativecommons.org/publicdomain/zero/1.0/"
    }
  ],
  "private": false,
  "scripts": {
    "rtl": "npm run rtl-sass",
    "rtl-sass": "node-sass --output-style nested --source-map true knice-rtl.scss css/knice-rtl.css",
    "build-sass": "node-sass --output-style nested --source-map true knice.scss css/knice.css",
    "scss-to-json": "scss-to-json scss/utils/_variables.scss",
    "stj": "npm run scss-to-json",
    "start": "npm run build-sass -- --watch"
  },
  "devDependencies": {
    "node-sass": "^4.14.1",
    "sass": "^1.26.11"
  },
  "dependencies": {
    "@octokit/rest": "^18.0.6",
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "clui": "^0.3.6",
    "configstore": "^5.0.1",
    "figlet": "^1.5.0",
    "inquirer": "^7.3.3",
    "lodash": "^4.17.20",
    "minimist": "^1.2.5",
    "scss-to-json": "^2.0.0",
    "shelljs": "^0.8.4",
    "simple-git": "^2.20.1",
    "touch": "^3.1.0"
  }
}
